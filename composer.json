{
    "name": "eppak/laradock-cli",
    "description": "Laradock cli",
    "version": "0.0.25",
    "keywords": [
        "laradock",
        "laravel",
        "docker",
        "console",
        "cli"
    ],
    "homepage": "https://cappellozza.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/eppak/laradock-cli/issues",
        "source": "https://github.com/eppak/laradock-cli"
    },
    "authors": [
        {
            "name": "Alessandro Cappellozza",
            "email": "alessandro.cappellozza@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-json": "*",
        "ext-posix": "*",
        "illuminate/log": "^7.0",
        "laminas/laminas-text": "^2.7",
        "laravel-zero/framework": "^7.0",
        "padraic/phar-updater": "^1.0.6",
        "symfony/process": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Eppak\\": "src/"
        },
        "files": [
            "src/Constants.php",
            "src/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "laradock-cli"
    ]
}
